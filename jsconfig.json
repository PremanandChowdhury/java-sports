{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2016",
    "jsx": "preserve",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"],
      "@components/*": ["components/*"],
      "@hooks/*": ["hooks/*"],
      "@services/*": ["services/*"],
      "@pages/*": ["pages/*"],
      "@utils/*": ["utils/*"],
      "@assets/*": ["assets/*"],
      "@store/*": ["store/*"]
    },
    "checkJs": true
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ]
}
